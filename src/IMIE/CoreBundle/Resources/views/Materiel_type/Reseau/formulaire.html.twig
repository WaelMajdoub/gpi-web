{# src/OC/PlatformBundle/Resources/views/Reseau/formulaire.html.twig #}

{% extends "IMIECoreBundle:formulaire.html.twig" %}
            

  
  {% block  group %} 
    
      <div class="form-group">
          <label for="nom">Type :</label>
          {{ form_errors(form.type) }}
          {{ form_widget(form.type, {'attr': {'class': 'form-control'}}) }}
      </div>
      <div class="form-group">
          <label for="nom">Nom dufabricant :</label>
          {{ form_errors(form.constructeur) }}
          {{ form_widget(form.constructeur, {'attr': {'class': 'form-control'}}) }}
      </div>
      <div class="form-group">
          <label for="nom">Model :</label>
          {{ form_errors(form.nom) }}
          {{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
      </div>
      <div class="form-group">
          <label for="nom">Nombre de ports :</label>
          {{ form_errors(form.port) }}
          {{ form_widget(form.port, {'attr': {'class': 'form-control'}}) }}
      </div>      
      <div class="form-group">
          <label for="nom">Vitesse (Gb/s)</label>
          {{ form_errors(form.vitesse) }}
          {{ form_widget(form.vitesse, {'attr': {'class': 'form-control'}}) }}
      </div> 
  {% endblock  %}