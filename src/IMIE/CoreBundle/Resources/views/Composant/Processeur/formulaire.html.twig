{# src/OC/PlatformBundle/Resources/views/composant/socket/formulaire/form.html.twig #}

{% extends "IMIECoreBundle:formulaire.html.twig" %}
            

  
  {% block  group %}  
      <div class="form-group">
          <label >Nom :</label>
          {{ form_errors(form.nom) }}
          {{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
      </div>
      <div class="form-group">
          <label >Constructeur :</label>
          {{ form_errors(form.constructeur) }}
          {{ form_widget(form.constructeur, {'attr': {'class': 'form-control'}}) }}
      </div>
      <div class="form-group">
          <label >Socket :</label>
          {{ form_errors(form.socket) }}
          {{ form_widget(form.socket, {'attr': {'class': 'form-control'}}) }}
      </div>
      <div class="form-group">
          <label >Vitesse :</label>
          {{ form_errors(form.cadence) }}
          {{ form_widget(form.cadence, {'attr': {'class': 'form-control'}}) }}
      </div>               

  {% endblock  %}